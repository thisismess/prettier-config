{
	"$schema": "http://json.schemastore.org/prettierrc",
	"printWidth": 120,
	"tabWidth": 1,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"arrowParens": "always",
	"trailingComma": "es5",
	"proseWrap": "always",
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": true,
	"endOfLine": "lf",
	"embeddedLanguageFormatting": "auto"
}
